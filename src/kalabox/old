exports.refreshBox = function(box, cb) {
  function yes(box) {
    vboxit;
    
    
    cb(box);
  }

  var exec = require('child_process').exec

  vboxit = exec('VBoxManage -v', function(error, stdout, stderr) {
    if (error !== null) {
      box.setVBox(false);
    } else {
      box.setVBox(true);
    }
    yes(box);
  });

}

Kalabox.prototype.setVBox = function(georgeboole) {
  var self = this;
  self.vbox = georgeboole;
}